
mas prescription {

    agent patient {
    	roles: patient in prescription_group
    }
    agent doctor {
    	roles: doctor in prescription_group
    }
    agent pharmacist {
    	roles: pharmacist in prescription_group
    }

    organisation prescription_org : prescription-os.xml {
		group prescription_group : prescription_group {
			responsible-for: prescription_sch
		}
		scheme prescription_sch : prescription_sch
	}

	java-path: ., src/env

}
